<packet>
    <name>IPv4</name>
    <header size="20">
        <!-- type: {
            0: Dropdown
            1: Hardware Address
            2: IPv4
            3: IPv6
            4: Int
        } -->
        <element name="Version/Header Length" size="1" type="0">
            <type name="4/5" val="0x45"/>
        </element>
        <element name="DSCP/ECN" size="1" type="0">
            <type name="Default" val="0x00"/>
        </element>
        <element name="Total Length" size="2" default="0" type="4"/>
        <element name="Identification" size="2" default="0" type="4"/>

        <element name="Hardware address destination" size="6" default="00:00:00:00:00:00" type="1"/>
        <element name="Hardware address source" size="6" default="00:00:00:00:00:00" type="1"/>
        <element name="Ethertype" size="2" type="0">
            <type name="IPv4" val="0x0800"/>
            <type name="ARP" val="0x0806"/>
            <type name="IPv6" val="0x86DD"/>
        </element>
    </header>
</packet>